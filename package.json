{
  "name": "open-in-developer-mode",
  "version": "0.3.0",
  "description": "Re-open the current file/project in developer mode",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/atom-open-in-developer-mode"
  },
  "keywords": [
    "devmode",
    "developer mode"
  ],
  "main": "./lib/main",
  "activationCommands": {
    "atom-workspace": [
      "open-in-developer-mode:toggle",
      "open-in-developer-mode:open"
    ]
  },
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "devDependencies": {
    "coffeelint-stylish": "^0.1.2",
    "gulp": "^4.0.0",
    "gulp-coffeelint": "^0.6.0",
    "gulp-debug": "^3.1.0",
    "gulp-jsonlint": "^1.2.0",
    "husky": "^0.14.3"
  },
  "scripts": {
    "test": "gulp lint"
  }
}
