{
  "name": "open-in-developer-mode",
  "version": "0.2.0",
  "description": "Re-open the current file/project in developer mode",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/atom-open-in-developer-mode"
  },
  "keywords": [
    "devmode",
    "developer mode"
  ],
  "main": "./lib/main",
  "activationCommands": {
    "atom-workspace": [
      "open-in-developer-mode:toggle",
      "open-in-developer-mode:open"
    ]
  },
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "coffeelint-stylish": "^0.1.2",
    "eslint": "^3.16.1",
    "eslint-config-atom-build": "^4.0.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-coffeelint": "^0.6.0",
    "gulp-debug": "^3.1.0",
    "gulp-jshint": "^2.0.4",
    "gulp-jsonlint": "^1.2.0",
    "gulp-lesshint": "^3.0.0",
    "jshint": "^2.9.4"
  },
  "scripts": {
    "test": "gulp lint"
  }
}
